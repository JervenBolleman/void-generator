@prefix ex: <https://purl.expasy.org/sparql-examples/void-generator/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:distinct_types_in_a_graph a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Count the number of triples with a prebound type."^^rdf:HTML ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """SELECT
  DISTINCT
    ?clazz
WHERE { 
  GRAPH ?graph {
    ?thing a ?clazz .
    ###REPLACE###
  }
}""" ;
    schema:keywords "void" , "predicate", "type" ;
    schema:target [
        a owl:Class ;
        rdfs:subClassOf [
            a owl:Restriction ;
            owl:onProperty rdf:type ;
            owl:someValuesFrom sd:Service
        ]
    ] .
