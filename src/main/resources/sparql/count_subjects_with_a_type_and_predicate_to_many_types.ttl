@prefix ex: <https://purl.expasy.org/sparql-examples/void-generator/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:count_subjects_with_a_type_and_predicate_to_many_types a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Count the number of predicate linksets between types in one graph."^^rdf:HTML ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """SELECT
  ?targetType 
  (COUNT (?subject) AS ?subjects) 
WHERE { 
  GRAPH ?graph { 
    ?subject a ?sourceType ; 
      ?predicate ?target . 
    ?target a ?targetType .
    FILTER(isIri(?targetType))
  }
} GROUP BY ?targetType
""" ;
    schema:keywords "void" , "predicate", "count", "type" ;
    schema:target [
        a sd:Service
    ] .
