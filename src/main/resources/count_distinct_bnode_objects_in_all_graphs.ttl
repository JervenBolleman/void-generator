@prefix ex: <https://purl.expasy.org/sparql-examples/void-generator/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:count_distinct_bnode_objects_in_all_graphs a sh:SPARQLExecutable,
        sh:SPARQLSelectExecutable ;
    rdfs:comment "Count the number of distinct bnodes used in the object position of triples in per named graph"^^rdf:HTML ;
    sh:prefixes _:sparql_examples_prefixes ;
    sh:select """
SELECT
    ?graph (COUNT(DISTINCT ?object) AS ?objects)
WHERE {
    GRAPH ?graph {
        ?subject ?predicate ?object .
        FILTER(isBlank(?object))
    }
} GROUP BY ?graph""" ;
    schema:keywords "void" , "blank node", "count" ;
    schema:target [
        a sd:Service
    ] .
